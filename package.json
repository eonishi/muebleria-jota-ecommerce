{
  "name": "muebleria-jota-ecommerce",
  "version": "1.0.0",
  "description": "Ecommerce de Muebleria Jota",
  "private": true,
  "workspaces": [
    "client",
    "backend",
    "schemas"
  ],
  "scripts": {
    "dev": "concurrently 'npm run back' 'npm run client'",
    "back": "npm run dev --workspace=backend",
    "client": "npm run dev --workspace=client",
    "build": "npm run build --workspace=client",
    "start": "npm run build && npm run start --workspace=backend"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marimilhas/muebleria-jota-ecommerce.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marimilhas/muebleria-jota-ecommerce/issues"
  },
  "homepage": "https://github.com/marimilhas/muebleria-jota-ecommerce#readme",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "standard": "^17.1.2"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  }
}
